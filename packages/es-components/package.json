{
  "name": "es-components",
  "version": "21.7.0-pre-prod.1",
  "description": "React components built for Exchange Solutions products",
  "author": "Willis Towers Watson - Individual Marketplace",
  "license": "MIT",
  "repository": "https://github.com/wtw-im/es-components",
  "module": "lib/index.js",
  "main": "cjs/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "MAIN_BUILD=\"$( (git merge-base --is-ancestor HEAD origin/main && echo 'true') || echo 'false' )\" npm run rollup",
    "lint": "eslint src",
    "mkdocsdir": "node -e \"let mdkirp = require('mkdirp'); let path = require('path'); mdkirp(path.join(__dirname, 'docs'));\" && touch docs/temp ",
    "prepublishOnly": "npm run build",
    "rollup": "rollup -c rollup.config.mjs",
    "styleguide-build": "npm run mkdocsdir && styleguidist build && rollup -c docs-rollup.config.mjs",
    "start": "concurrently --kill-others \"npm run mkdocsdir && rollup -c docs-rollup.config.mjs -w\" \"styleguidist server\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "symlink-dir ../../node_modules/mochawesome ./node_modules/mochawesome",
    "wait-for-start": "wait-on http://localhost:6060",
    "cypressreport": "mochawesome-merge mochawesome-report/*.json > raw-results.json && marge --reportDir cypress-results raw-results.json"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFiles": [
      "./config/jest-shims.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/import-matchers.js",
      "<rootDir>/config/jest-setup.ts"
    ],
    "testPathIgnorePatterns": [
      "es6",
      "lib"
    ],
    "testEnvironmentOptions": {
      "url": "http://localhost/"
    },
    "testRegex": "specs\\.(js|ts|tsx)$",
    "transform": {
      "^.+\\.(j|t)sx?$": [
        "ts-jest",
        {
          "tsconfig": "<rootDir>/config/test-tsconfig.json"
        }
      ]
    },
    "moduleNameMapper": {
      "^es-components-via-theme$": "<rootDir>/../es-components-via-theme/src/index.ts"
    },
    "preset": "ts-jest"
  },
  "browserslist": "> 0.5%, not dead",
  "peerDependencies": {
    "react": "^18",
    "react-dom": "^18",
    "styled-components": "^5"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.7",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/eslint-plugin": "^7.19.1",
    "@babel/plugin-proposal-export-default-from": "^7.18.10",
    "@babel/plugin-proposal-optional-chaining": "^7.20.7",
    "@babel/plugin-transform-modules-commonjs": "^7.20.7",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime-corejs3": "^7.20.7",
    "@jest/globals": "^29.3.1",
    "@oat-sa/rollup-plugin-wildcard-external": "^0.1.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.2.1",
    "@rollup/plugin-typescript": "^10.0.1",
    "@rollup/pluginutils": "^5.0.2",
    "@testing-library/cypress": "^8.0.7",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.17",
    "@types/styled-components": "^5.1.26",
    "@types/tinycolor2": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "ast-types": "^0.14.2",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-preset-wtw-im": "^2.5.0",
    "bl": "^6.0.0",
    "braces": "^3.0.2",
    "concurrently": "^7.6.0",
    "cypress": "^9.7.0",
    "es-components-via-theme": "^21.7.0-pre-prod.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.7",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "jest": "^29.3.1",
    "jest-cli": "^29.3.1",
