{
  "name": "es-components",
  "version": "5.0.0",
  "description": "React components built for Exchange Solutions products",
  "repository": "https://github.com/TWExchangeSolutions/es-components",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src -d lib --ignore specs.js,*.md --copy-files",
    "ci": "npm run lint && npm test",
    "lint": "eslint src",
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "prettify": "prettier --single-quote --write \"src/**/*.js\"",
    "styleguide-build": "styleguidist build",
    "styleguide-server": "styleguidist server",
    "test": "jest"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --single-quote --write",
      "eslint --fix",
      "git add"
    ]
  },
  "author": "Exchange Solutions",
  "license": "MIT",
  "jest": {
    "setupTestFrameworkScriptFile": "./config/jest-setup.js",
    "testRegex": "specs\\.js$"
  },
  "peerDependencies": {
    "react": ">=0.14"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.9",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "classnames": "^2.2.5",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "enzyme": "^2.6.0",
    "eslint": "^2.13.1",
    "eslint-config-exchange-solutions": "^6.0.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-react": "^4.3.0",
    "file-loader": "^0.9.0",
    "husky": "^0.13.3",
    "jest": "^18.0.0",
    "jest-cli": "^20.0.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lint-staged": "^3.4.1",
    "mockdate": "^2.0.1",
    "prettier": "^1.3.1",
    "prop-types": "^15.5.6",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-styleguidist": "^5.0.6",
    "react-test-renderer": "^15.4.2",
    "style-loader": "^0.13.1",
    "webpack": "^2.3.3"
  },
  "dependencies": {
    "babel-plugin-lodash": "^3.2.11",
    "babel-runtime": "^6.20.0",
    "classnames": "^2.2.5",
    "date-fns": "^1.28.3",
    "dom-helpers": "^3.2.1",
    "focus-trap-react": "^3.0.3",
    "lodash": "^4.17.4",
    "react-animate-height": "^0.9.11",
    "react-overlays": "^0.7.0",
    "styled-components": "^2.0.0",
    "tinycolor2": "^1.4.1",
    "uncontrollable": "^4.1.0"
  }
}
