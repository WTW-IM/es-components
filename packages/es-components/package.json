{
  "name": "es-components",
  "version": "21.6.4",
  "description": "React components built for Exchange Solutions products",
  "author": "Willis Towers Watson - Individual Marketplace",
  "license": "MIT",
  "repository": "https://github.com/wtw-im/es-components",
  "module": "lib/index.js",
  "main": "cjs/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "npm run rollup",
    "ci": "npm run lint && npm test",
    "lint": "eslint src",
    "mkdocsdir": "node -e \"let mdkirp = require('mkdirp'); let path = require('path'); mdkirp(path.join(__dirname, 'docs'));\"",
    "prepublishOnly": "npm run build",
    "rollup": "rollup -c rollup.config.js",
    "styleguide-build": "npm run mkdocsdir && styleguidist build && rollup -c docs-rollup.config.js",
    "start": "concurrently --kill-others \"npm run mkdocsdir && rollup -c docs-rollup.config.js -w\" \"styleguidist server\"",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "jest": {
    "setupFiles": [
      "./config/jest-shims.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/import-matchers.js"
    ],
    "testPathIgnorePatterns": [
      "es6",
      "lib"
    ],
    "testRegex": "specs\\.js$",
    "testURL": "http://localhost/"
  },
  "browserslist": "> 0.5%, not dead",
  "prettier": {
    "singleQuote": true
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8",
    "styled-components": ">=4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/runtime-corejs3": "^7.5.1",
    "@oat-sa/rollup-plugin-wildcard-external": "^0.1.0",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/pluginutils": "^3.1.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^10.4.6",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-preset-wtw-im": "^2.5.0",
    "concurrently": "^5.2.0",
    "es-components-via-theme": "file:../es-components-via-theme",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-exchange-solutions": "^7.0.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "http-server": "^0.12.3",
    "jest": "^26.1.0",
    "jest-cli": "^26.1.0",
    "jest-styled-components": "^7.0.2",
    "mkdirp": "^1.0.4",
    "prettier": "^1.18.2",
    "react": "^16.13.1",
    "react-docgen": "^5.3.0",
    "react-docgen-displayname-handler": "^3.0.1",
    "react-dom": "^16.13.1",
    "react-styleguidist": "^11.0.8",
    "react-test-renderer": "^16.8.6",
    "react-testing-library": "^6.1.2",
    "rollup": "^2.21.0",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-generate-html-template": "^1.6.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^6.1.0",
    "styled-components": "^5.1.1",
    "webpack": "^4.44.1"
  },
  "dependencies": {
    "@babel/helpers": "^7.5.0",
    "@babel/runtime": "^7.5.0",
    "core-js": "^3.1.4",
    "date-fns": "^2.4.1",
    "format-message": "^6.2.3",
    "get-root-node-polyfill": "^1.0.0",
    "lodash": "^4.17.19",
    "prop-types": "^15.7.2",
    "react-animate-height": "^2.0.15",
    "react-datepicker": "^2.9.6",
    "react-modal": "^3.11.2",
    "react-overlays": "^2.1.0",
    "react-popper": "^1.3.3",
    "react-text-mask": "^5.4.3",
    "react-transition-group": "^4.1.1",
    "text-mask-addons": "3.7.2",
    "tinycolor2": "^1.4.1"
  }
}
