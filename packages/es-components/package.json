{
  "name": "es-components",
  "version": "21.7.0-pre-prod.4",
  "description": "React components built for Exchange Solutions products",
  "author": "Willis Towers Watson - Individual Marketplace",
  "license": "MIT",
  "repository": "https://github.com/wtw-im/es-components",
  "module": "lib/index.js",
  "main": "cjs/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "cross-env MAIN_BUILD=\"$( (git merge-base --is-ancestor HEAD origin/main && echo 'true') || echo 'false' )\" npm run rollup",
    "lint": "eslint src",
    "mkdocsdir": "node -e \"let mdkirp = require('mkdirp'); let path = require('path'); mdkirp(path.join(__dirname, 'docs'));\" && touch docs/temp ",
    "prepublishOnly": "npm run build",
    "rollup": "rollup -c rollup.config.js",
    "styleguide-build": "npm run mkdocsdir && styleguidist build && rollup -c docs-rollup.config.js",
    "pre-prod-sg-build": "styleguidist build --config ./pre-prod-styleguide.config.js && npm run pre-prod-fc-icons",
    "pre-prod-fc-icons": "cp docs/full-color-icons.html docs/pre-prod && cp docs/full-color-icons.js docs/pre-prod",
    "start": "concurrently --kill-others \"npm run mkdocsdir && rollup -c docs-rollup.config.js -w\" \"styleguidist server\"",
    "test": "npm run reinstall-via-theme && jest",
    "reinstall-via-theme": "npm uninstall es-components-via-theme && npm install ../es-components-via-theme",
    "test:watch": "jest --watch",
    "wait-for-start": "wait-on http://localhost:6060",
    "cypressreport": "mochawesome-merge mochawesome-report/*.json > raw-results.json && marge --reportDir cypress-results raw-results.json"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFiles": [
      "./config/jest-shims.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/import-matchers.js"
    ],
    "testPathIgnorePatterns": [
      "es6",
      "lib"
    ],
    "testEnvironmentOptions": {
      "url": "http://localhost/"
    },
    "testRegex": "specs\\.js$"
  },
  "browserslist": "> 0.5%, not dead",
  "peerDependencies": {
    "react": "^16.8 || ^17 || ^18",
    "react-dom": "^16.8 || ^17 || ^18",
    "styled-components": "^4 || ^5"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.13",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/eslint-plugin": "^7.18.10",
    "@babel/plugin-proposal-export-default-from": "^7.12.13",
    "@babel/plugin-proposal-optional-chaining": "^7.13.12",
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "@babel/runtime-corejs3": "^7.17.9",
    "@oat-sa/rollup-plugin-wildcard-external": "^0.1.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/pluginutils": "^4.1.0",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "ast-types": "^0.14.2",
    "babel-jest": "^29.0.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^2.0.7",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-preset-wtw-im": "^2.5.0",
    "bl": "^1.2.3",
    "braces": "^3.0.2",
    "concurrently": "^6.0.2",
    "cross-env": "^7.0.3",
    "cypress": "^9.6.0",
    "es-components-via-theme": "file:../es-components-via-theme",
    "eslint": "^8.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^27.0.1",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.31.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.6.0",
    "jest": "^27.5.1",
    "jest-cli": "^27.3.1",
    "jest-styled-components": "^7.0.8",
    "mkdirp": "^1.0.4",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0",
    "prettier": "^2.2.1",
    "react": "^18.2.0",
    "react-docgen": "^5.3.1",
    "react-docgen-displayname-handler": "^3.0.2",
    "react-dom": "^18.2.0",
    "react-styleguidist": "^13.0.0",
    "react-test-renderer": "^17.0.2",
    "rollup": "^2.45.2",
    "rollup-plugin-generate-html-template": "^1.7.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "styled-components": "^5.3.5",
    "wait-on": "^6.0.1",
    "webpack": "^5.74.0"
  },
  "dependencies": {
    "@babel/helpers": "^7.13.16",
    "@babel/runtime": "^7.17.9",
    "@popperjs/core": "^2.9.2",
    "core-js": "^3.10.2",
    "date-fns": "^2.21.1",
    "format-message": "^6.2.3",
    "get-root-node-polyfill": "^1.0.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "react-animate-height": "^2.1.2",
    "react-datepicker": "^3.8.0",
    "react-is": "^18.2.0",
    "react-modal": "^3.14.4",
    "react-overlays": "^5.0.0",
    "react-popper": "^2.2.5",
    "react-text-mask": "^5.4.3",
    "react-transition-group": "^4.4.1",
    "text-mask-addons": "3.8.0",
    "tinycolor2": "^1.4.2"
  }
}
