    "@floating-ui/react": "^0.14.0",
    "@popperjs/core": "^2.11.6",
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-modal": "^3.13.1",
    "@types/react-transition-group": "^4.4.5",
    "core-js": "^3.26.1",
    "csstype": "^3.1.1",
